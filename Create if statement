import java.util.*;
class Square{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a no :");
        int n=sc.nextInt();
        if(n%5==0){
            int square=n*n;
            System.out.println("The square of no which is divisible by 5 : "+square);
        }
 }
}
