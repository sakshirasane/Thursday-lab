import java.util.*;
class Square{
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a age of person :");
        int n=sc.nextInt();
        if(n>=18){
System.out.println("The person is eligible for vote : "+n);
        }
        else{
            System.out.println("The person is not eligible for vote");
        }
    }
}
